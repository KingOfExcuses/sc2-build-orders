// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Build {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  matchup String
  title   String
  build   String
}
